    /**
     * @var \{{ entityClass }} ${{ singularName }}
     */
     public ${{ singularName }};
{%- set belongsTo = Bake.aliasExtractor(modelObj, 'BelongsTo') %}
{%- set belongsToMany = Bake.aliasExtractor(modelObj, 'belongsToMany') %}
{%- for assoc in belongsTo|merge(belongsToMany) %}
    {%- set otherModel = PageBake.getAssociatedTable(modelObj, assoc) %}
    {%- set classInfo = PageBake.getOjectFqn(otherModel) %}
    {%- set otherName = Bake.getAssociatedTableAlias(modelObj, assoc) %}
    {%- set otherPlural = otherName|variable %}


    /**
     * @var \{{ classInfo ?? 'Cake\\Datasource\\EntityInterface' }} ${{ otherPlural }}
     */
     public ${{ otherPlural }};
{%- endfor %}